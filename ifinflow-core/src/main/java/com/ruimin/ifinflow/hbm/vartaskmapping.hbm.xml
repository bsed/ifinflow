<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ruimin.ifinflow.model.flowmodel.xml.VarTaskMapping" table="IFF_VARTASKMAPPING">
        <id name="id" type="string">
            <column name="POID" length="32" />
            <generator class="assigned" />
        </id>
        <property name="nodeHandle" type="string">
            <column name="NODEHANDLE" length="32" not-null="true" />
        </property>
        <property name="variableName" type="string">
            <column name="VARIABLENAME" length="64" not-null="true" />
        </property>
        <property name="userExtColume" type="string">
            <column name="USEREXTCOLUME" length="64" />
        </property>
    </class>
</hibernate-mapping>
